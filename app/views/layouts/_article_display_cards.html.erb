
<div class="columns">
    <% 3.times do %>
        <div class="column col-4">
    <% 2.times do %>
            <div class="card mb-3">
                <div class="card-image">
                    <img src="img/osx-el-capitan.jpg" class="img-responsive">
                    </div>
                <div class="card-header">
                    <div class="card-title h5">Article Title</div>
                    <div class="card-subtitle text-gray">Author</div>
                </div>
                <div class="card-body">
                   <p>Article Title</p>
                </div>
                <div class="card-footer">
                    <button class="btn btn-primary">Go to Article</button>
                </div>
            </div>
    <% end %>
        </div>
    <% end %>    
</div>
